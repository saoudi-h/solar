{
    "name": "@solar/react",
    "version": "0.0.0",
    "private": true,
    "description": "Solar Icons for React",
    "type": "module",
    "sideEffects": false,
    "scripts": {
        "build": "vite build && tsc",
        "lint": "eslint . --max-warnings 0",
        "lint:fix": "eslint . --fix",
        "format:check": "prettier \"**/*\" --ignore-unknown --list-different",
        "format:fix": "prettier \"**/*\" --ignore-unknown --write",
        "clean": "rimraf *.tsbuildinfo node_modules build dist",
        "typecheck": "tsc"
    },
    "module": "./dist/esm/index.mjs",
    "main": "./dist/cjs/index.cjs",
    "types": "./dist/types/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/esm/index.mjs",
            "require": "./dist/cjs/index.cjs",
            "types": "./dist/types/index.d.ts"
        },
        "./icons/*": {
            "import": "./dist/esm/csr/*.mjs",
            "require": "./dist/cjs/csr/*.cjs",
            "types": "./dist/types/csr/*.d.ts"
        },
        "./csr/*": {
            "import": "./dist/esm/csr/*.mjs",
            "require": "./dist/cjs/csr/*.cjs",
            "types": "./dist/types/csr/*.d.ts"
        },
        "./lib/*": {
            "import": "./dist/esm/lib/*.mjs",
            "require": "./dist/cjs/lib/*.cjs",
            "types": "./dist/types/lib/*.d.ts"
        },
        "./ssr": {
            "import": "./dist/esm/ssr/index.mjs",
            "require": "./dist/cjs/ssr/index.cjs",
            "types": "./dist/types/ssr/index.d.ts"
        },
        "./ssr/*": {
            "import": "./dist/esm/ssr/*.mjs",
            "require": "./dist/cjs/ssr/*.cjs",
            "types": "./dist/types/ssr/*.d.ts"
        },
        "./package.json": {
            "default": "./package.json"
        },
        "./*": {
            "import": "./dist/esm/csr/*.mjs",
            "require": "./dist/cjs/csr/*.cjs",
            "types": "./dist/types/csr/*.d.ts"
        }
    },
    "devDependencies": {
        "@solar/core": "workspace:*",
        "@solar/eslint": "workspace:*",
        "@solar/prettier": "workspace:*",
        "@solar/tsconfig": "workspace:*",
        "@types/node": "^22.8.7",
        "@types/react": "^18.3.12",
        "@types/react-dom": "^18.3.1",
        "@vitejs/plugin-react": "^4.3.3",
        "dotenv": "^16.4.5",
        "prettier": "^3.3.3",
        "react": "18.3.1",
        "react-dom": "^18.3.1",
        "rimraf": "^6.0.1",
        "tsx": "^4.19.2",
        "typescript": "^5.0.0",
        "vite": "^5.4.10",
        "vitest": "^2.1.3"
    },
    "peerDependencies": {
        "react": ">= 16.8",
        "react-dom": ">= 16.8"
    },
    "engines": {
        "node": ">=16"
    },
    "keywords": [
        "react",
        "icons",
        "svg",
        "solar",
        "design",
        "interface",
        "phosphor",
        "UI",
        "UX"
    ],
    "author": {
        "name": "Saoudi Hakim",
        "url": "https://hakimsaoudi.dev",
        "email": "saoudihakim@gmail.com"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/saoudi-h/solar/issues"
    }
}
